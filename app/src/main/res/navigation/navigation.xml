<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fristFragment">

    <fragment
        android:id="@+id/fristFragment"
        android:name="com.example.myapplication.ViewModel.FristFragment"
        android:label="fragment_frist"
        tools:layout="@layout/fragment_frist" >
        <action
            android:id="@+id/action_fristFragment_to_senondFragment"
            app:destination="@id/senondFragment" />
    </fragment>
    <fragment
        android:id="@+id/senondFragment"
        android:name="com.example.myapplication.ViewModel.SenondFragment"
        android:label="fragment_senond"
        tools:layout="@layout/fragment_senond" >
        <action
            android:id="@+id/action_senondFragment_to_thridFragment"
            app:destination="@id/thridFragment" />
    </fragment>
    <fragment
        android:id="@+id/thridFragment"
        android:name="com.example.myapplication.ViewModel.ThridFragment"
        android:label="fragment_thrid"
        tools:layout="@layout/fragment_thrid" />
</navigation>